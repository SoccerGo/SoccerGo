<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="heracles.soccergo.more.SettingActivity"
    android:background="#e5e5e5">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:background="#33a333"
        android:id="@+id/relativeLayout34"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true">

        <ImageView
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginTop="2dp"
            android:layout_marginLeft="3dp"
            android:src="@drawable/back_icon_left"
            android:id="@+id/ivBack" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="设  置"
            android:layout_centerVertical="true"
            android:layout_centerHorizontal="true"
            android:textColor="#ffffff"
            android:textStyle="bold"
            android:textSize="16dp" />
    </RelativeLayout>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/relativeLayout34"
        android:layout_alignStart="@+id/relativeLayout34"
        android:layout_marginTop="20dp"
        android:background="#fcfcfc"
        android:paddingLeft="20dp"
        android:paddingRight="20dp"
        android:id="@+id/linearLayout9">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="新消息提醒"
            android:id="@+id/textView40"
            android:layout_marginTop="12dp"
            android:layout_marginBottom="12dp"
            android:textSize="16dp"
            android:textColor="#131313" />

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#d4d4d4"></LinearLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="勿扰模式"
            android:id="@+id/textView69"
            android:layout_marginTop="12dp"
            android:layout_marginBottom="12dp"
            android:textSize="16dp"
            android:textColor="#131313" />

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#d4d4d4"
            android:layout_gravity="center_horizontal" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="隐私"
            android:id="@+id/textView71"
            android:layout_marginTop="12dp"
            android:layout_marginBottom="12dp"
            android:textSize="16dp"
            android:textColor="#131313" />

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#d4d4d4" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="更多设置"
            android:id="@+id/textView72"
            android:layout_marginTop="12dp"
            android:layout_marginBottom="12dp"
            android:textSize="16dp"
            android:textColor="#131313" />

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#d4d4d4" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="账号与安全"
            android:id="@+id/textView73"
            android:layout_marginTop="12dp"
            android:layout_marginBottom="12dp"
            android:textSize="16dp"
            android:textColor="#131313" />
    </LinearLayout>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="25dp"
        android:background="#fcfcfc"
        android:paddingLeft="20dp"
        android:paddingRight="20dp"
        android:layout_below="@+id/linearLayout9"
        android:layout_alignParentStart="true">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="关于足球小将"
            android:id="@+id/textView74"
            android:layout_marginTop="12dp"
            android:layout_marginBottom="12dp"
            android:textSize="16dp"
            android:textColor="#131313" />

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#d4d4d4" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="退出登录"
            android:id="@+id/tvExitLogin"
            android:layout_marginTop="12dp"
            android:layout_marginBottom="12dp"
            android:textSize="16dp"
            android:textColor="#131313" />

    </LinearLayout>
</RelativeLayout>
